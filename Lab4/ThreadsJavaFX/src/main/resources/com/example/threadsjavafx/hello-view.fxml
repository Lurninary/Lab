<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="212.0" prefWidth="326.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.threadsjavafx.Client.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ProgressBar fx:id="Pr" prefHeight="22.0" prefWidth="200.0" progress="0.0" />
   <HBox alignment="CENTER" prefHeight="39.0" prefWidth="286.0" spacing="50.0">
      <children>
         <Button mnemonicParsing="false" onAction="#Start" text="Start" />
         <Button fx:id="PauseResume" mnemonicParsing="false" onAction="#PauseResume" text="Pause" />
         <Button mnemonicParsing="false" onAction="#Stop" text="Stop" />
      </children>
      <opaqueInsets>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </opaqueInsets>
      <cursor>
         <Cursor fx:constant="DEFAULT" />
      </cursor>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </HBox>
   <Button mnemonicParsing="false" onAction="#ConsoleStart" text="Console" />
</VBox>
